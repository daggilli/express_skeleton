{
  "name": "express_skeleton",
  "version": "1.0.0",
  "description": "A template Express app using Typescript",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "start": "node --env-file .env target/dist/index.js",
    "build": "npm run lint && tsc",
    "buildq": "tsc",
    "check": "tsc --noEmit",
    "lint": "npx eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggilli/express_skeleton.git"
  },
  "imports": {
    "#*": "./src/*"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/daggilli/express_skeleton/issues"
  },
  "homepage": "https://github.com/daggilli/express_skeleton#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "eslint": "^9.28.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  }
}
